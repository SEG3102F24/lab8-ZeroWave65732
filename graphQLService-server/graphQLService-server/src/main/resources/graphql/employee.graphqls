extend type Query {
    employees(id: ID!): [Employee]
}

type Employee {
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Int!
    gender: String!
    email: String!
}

input CreateEmployeeInput {
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Int!
    gender: String!
    email: String!
}

extend type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!): Employee!
}
